<?xml version="1.0" ?>
<list xmlns="http://schemas.sulu.io/list-builder/list">
    <key>events</key>
    <properties>
        <property name="id" visibility="no" translation="sulu_admin.id">
            <field-name>id</field-name>
            <entity-name>Otd\SuluEventBundle\Entity\Event</entity-name>
        </property>
        <property name="name" visibility="always" searchability="yes" translation="sulu_admin.name">
            <field-name>name</field-name>
            <entity-name>Otd\SuluEventBundle\Entity\Event</entity-name>
        </property>

        <property name="startDate" visibility="yes" translation="otd_sulu.start_date" type="datetime">
            <field-name>startDate</field-name>
            <entity-name>Otd\SuluEventBundle\Entity\Event</entity-name>
        </property>

        <property name="endDate" visibility="yes" translation="otd_sulu.end_date" type="datetime">
            <field-name>endDate</field-name>
            <entity-name>Otd\SuluEventBundle\Entity\Event</entity-name>
        </property>

        <property name="created" visibility="no" translation="sulu_admin.created" type="datetime">
            <field-name>created</field-name>
            <entity-name>Otd\SuluEventBundle\Entity\Event</entity-name>
        </property>

        <property name="changed" visibility="no" translation="sulu_admin.changed" type="datetime">
            <field-name>changed</field-name>
            <entity-name>Otd\SuluEventBundle\Entity\Event</entity-name>
        </property>
    </properties>
</list>
